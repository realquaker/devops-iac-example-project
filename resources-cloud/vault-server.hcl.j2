storage "consul" {
  address = "127.0.0.1:8500"
  path    = "vault/"
}

listener "tcp" {
  address     = "0.0.0.0:8200"
  tls_disable = 1
}

api_addr = "http://{{ ansible_host }}:8200"
cluster_addr = "http://{{ ansible_host }}:8201"

seal "awskms" {
  region = "eu-central-1"
  kms_key_id = "your-kms-key-id"
}
